{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 9 projects with a relocated document root",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "homepage": "https://www.drupal.org/project/drupal",
  "support": {
    "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
    "chat": "https://www.drupal.org/node/314178"
  },
  "authors": [
    {
      "name": "IC Webteam",
      "email": "webteam@gatech.edu",
      "role": "Makers of things like this"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
        "type": "path",
        "url": "gt/gt_profile"
    },
    {
        "type": "path",
        "url": "gt/gt_theme"
    },
    {
        "type": "path",
        "url": "gt/gt_tools"
    },
    {
        "type": "path",
        "url": "gt/hg_reader"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "^1.9",
    "drupal/admin_toolbar": "^3.0",
    "drupal/block_region_permissions": "^1.4",
    "drupal/captcha": "^2.0",
    "drupal/cas": "^2.0",
    "drupal/ckeditor": "^1.0",
    "drupal/classy": "^1.0",
    "drupal/conditional_fields": "^4.0@alpha",
    "drupal/core-composer-scaffold": "^10.0",
    "drupal/core-project-message": "^10.0",
    "drupal/core-recommended": "^10.0",
    "drupal/ctools": "^4.0",
    "drupal/custom_search": "^1.0@beta",
    "drupal/diff": "^1.1",
    "drupal/externalauth": "^2.0",
    "drupal/faqfield": "^7.0",
    "drupal/field_formatter_class": "^1.4",
    "drupal/field_group": "^3.4",
    "drupal/fpa": "^4.0",
    "drupal/genpass": "^2.0",
    "drupal/imce": "^3.0",
    "drupal/inline_entity_form": "^2.0@beta",
    "drupal/jquery_ui_accordion": "^2.0",
    "drupal/jquery_ui_slider": "^2.0",
    "drupal/layout_builder_modal": "^1.1",
    "drupal/layout_builder_restrictions": "^2.7",
    "drupal/libraries": "^4.0",
    "drupal/linkit": "^6.0",
    "drupal/menu_block": "^1.10",
    "drupal/menu_breadcrumb": "^2.0@alpha",
    "drupal/migrate_plus": "^6.0",
    "drupal/migrate_skip_on_404": "^1.1",
    "drupal/migrate_upgrade": "^4.0",
    "drupal/pathauto": "^1.11",
    "drupal/quickedit": "^1.0",
    "drupal/rabbit_hole": "^1.0@beta",
    "drupal/rdf": "^2.0",
    "drupal/redirect": "^1.8",
    "drupal/seven": "^1.0",
    "drupal/social_media_links": "^2.7",
    "drupal/stable": "^2.0",
    "drupal/token": "^1.11",
    "drupal/video_embed_field": "^2.5",
    "drupal/video_filter": "^1.2",
    "drupal/views_accordion": "^2.0",
    "drupal/views_autocomplete_filters": "^1.4",
    "drupal/views_data_export": "^1.3",
    "drupal/views_field_view": "^1.0@beta",
    "drupal/views_flipped_table": "^2.0",
    "drupal/views_taxonomy_term_name_into_id": "^1.0@RC",
    "drupal/webform": "^6.1",
    "drush/drush": "^11.0",
    "gt/gt_profile": "*",
    "gt/gt_theme": "*",
    "gt/gt_tools": "*",
    "gt/hg_reader": "*",
    "platformsh/config-reader": "^2.4",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3"
  },
  "conflict": {
      "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/console-extend-plugin": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "drupal-core-project-message": {
      "include-keys": [
        "homepage",
        "support"
      ]
    }
  }
}
